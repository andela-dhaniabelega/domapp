{
  "name": "domapp",
  "version": "1.0.0",
  "description": "Document Management App",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --watch server --exec babel-node -- ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-test": "npm run refill-testdb && NODE_ENV=test mocha --compilers js:babel-core/register server/test/**/*.test.js",
    "reset-testdb": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test 5sequelize db:migrate",
    "seed-testdb": "NODE_ENV=test sequelize db:seed:all",
    "refill-testdb": "npm run reset-testdb && npm run seed-testdb"
  },
